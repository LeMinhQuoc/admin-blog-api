{
    "name": "kunstmaan/sensio-generator-bundle",
    "description": "This bundle generates code for you",
    "keywords": [],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/framework-bundle": "^3.4|^4.4|^5.1",
        "symfony/console": "^3.4|^4.4|^5.1",
        "symfony/process": "^3.4|^4.4|^5.1",
        "symfony/yaml": "^3.4|^4.4|^5.1",
        "twig/twig": "^1.28.2|^2.0"
    },
    "require-dev": {
        "symfony/doctrine-bridge": "^3.4|^4.4|^5.1",
        "symfony/filesystem": "^3.4|^4.4|^5.1",
        "symfony/phpunit-bridge": "^5.1",
        "doctrine/orm": "^2.6",
        "doctrine/common": "^2.11",
        "doctrine/persistence": "^1.3"
    },
    "autoload": {
        "psr-4": { "Sensio\\Bundle\\GeneratorBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "replace": {
        "sensio/generator-bundle": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.3.x-dev"
        }
    }
}
